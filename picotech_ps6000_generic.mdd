                  <MCode>function status = ps6000SetExternalClock(obj, frequency, voltage)
% For group functions, OBJ is the group object. For
% base device functions, OBJ is the device object.

    if (abs(voltage) &gt;= 1)
        
        error(['PS6000IDSetExternalClock:InvalidThreshold', ...
            'Please choose threshold value in the range ±1 V\n']);
        
    elseif(frequency &lt; 0 || frequency &gt; 4)
        
        error(['PS6000IDSetExternalClock:InvalidFrequency', ... 
            'Please choose frequency enumeration value between ' ...
            '0 (PS6000_FREQUENCY_5MHZ) and 4 (PS6000_FREQUENCY_25MHZ)']);
        
    end
    
    threshold = voltage * 32512;
    
    status = calllib('ps6000', 'ps6000SetExternalClock', ...
        obj.DriverData.unitHandle, frequency, threshold);
    
    if (status == PicoStatus.PICO_INVALID_CHANNEL)       
            
        error('ps6000SetChannel: error code %d\n', status);
            
    end</MCode>
               </Code>
