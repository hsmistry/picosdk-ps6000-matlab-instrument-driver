      <Group>
         <Name>block</Name>
         <Description>The Block group contains a set of properties and functions related to the block data collection functionality of the device.</Description>
         <Command/>
         <Size>1</Size>
         <Mappings>
            <ChannelMap Command="block" Index="1"/>
         </Mappings>
